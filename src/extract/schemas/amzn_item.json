{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "brand": {
      "type": "string",
      "description": "Brand name of the product"
    },
    "price": {
      "type": "string",
      "description": "Price of the product"
    },
  
    "title": {
      "type": "string",
      "description": "Title of the product"
    },
    "page": {
      "type": "integer",
      "description": "Page number from which the product was scraped"
    }
  },
    "required": [
        "brand", 
        "price", 
        "title", 
        "page"
    ]
}
